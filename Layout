            $(document).ready(
                    function () {
                        setInterval(function () {
                            $.get("ajax_updates.php", function (result) {
                                var results = result.split("|");
                                $(".inmates").html(results[0]);
                                $(".mail").html(results[1]);
                                $(".events").html(results[2]);
                                $(".hitlist").html(results[3]);
                                $(".animals").html(results[4]);
								if(results[5] > 0)
									document.title = "(" + results[5] + ") Mafia Thug";
								else if(results[5] == 0)
									document.title = "Mafia Thug";
                                $(".genBars").html(results[6]);
                            });
                        }, 2000);
                    });
        </script>
